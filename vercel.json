{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[...all].js"
    },
    {
      "src": "/",
      "dest": "/auth.html"
    },
    {
      "src": "/login",
      "dest": "/auth.html"
    },
    {
      "src": "/admin",
      "dest": "/index.html"
    },
    {
      "src": "/client",
      "dest": "/Client/dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}