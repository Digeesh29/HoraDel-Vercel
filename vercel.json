{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/login", "dest": "/auth.html" },
    { "src": "/admin", "dest": "/index.html" },
    { "src": "/client", "dest": "/Client/dashboard.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}