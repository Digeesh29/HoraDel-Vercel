{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "Client/**", "use": "@vercel/static" },
    { "src": "pages/**", "use": "@vercel/static" },
    { "src": "css/**", "use": "@vercel/static" },
    { "src": "js/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/dashboard/summary", "dest": "/api/dashboard.js" },
    { "src": "/api/dashboard", "dest": "/api/dashboard.js" },
    { "src": "/api/bookings", "dest": "/api/bookings.js" },
    { "src": "/api/vehicles", "dest": "/api/vehicles.js" },
    { "src": "/api/companies", "dest": "/api/companies.js" },
    { "src": "/api/ratecards", "dest": "/api/ratecards.js" },
    { "src": "/api/reports/(.*)", "dest": "/api/reports.js" },
    { "src": "/api/reports", "dest": "/api/reports.js" },
    { "src": "/api/auth/(.*)", "dest": "/api/auth.js" },
    { "src": "/api/auth", "dest": "/api/auth.js" },
    { "src": "/api/simple-test", "dest": "/api/simple-test.js" },
    { "src": "/api/add-sample-data", "dest": "/api/add-sample-data.js" },
    { "src": "/api/debug", "dest": "/api/debug.js" },
    { "src": "/login", "dest": "/auth.html" },
    { "src": "/test-auth", "dest": "/test-auth.html" },
    { "src": "/quick-test", "dest": "/quick-test.html" },
    { "src": "/admin", "dest": "/index.html" },
    { "src": "/client", "dest": "/Client/dashboard.html" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}